{
  "name": "interoke2020",
  "version": "1.0.0",
  "private": true,
  "description": "Interoke Digital static site (Jigsaw)",
  "scripts": {
    "dev": "pnpm run development",
    "development": "pnpm exec mix",
    "watch": "pnpm exec mix watch",
    "hot": "pnpm exec mix watch --hot",
    "production": "pnpm exec mix --production && ./jigsaw-php vendor/bin/jigsaw build production",
    "build": "pnpm run production",
    "serve": "./jigsaw-php vendor/bin/jigsaw serve --port=8001"
  },
  "packageManager": "pnpm@8.15.0",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.38",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.77.8",
    "sass-loader": "^13.3.2",
    "webpack": "^5.104.1",
    "webpack-cli": "^4.9.1"
  }
}
